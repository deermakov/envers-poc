type Deal {
    id: ID!
    num: String!
    sum: Float
    participants: [LegalEntity]
}

type LegalEntity {
    id: ID!
    inn: String!
}

# The Root Query for the application
type Query {
    allDeals: [Deal]!
    allLegalEntities: [LegalEntity]!
}

# The Root Mutation for the application
type Mutation {
    writeDeal(num: String!, sum: Float) : Deal!
}